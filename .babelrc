{
    "presets": [
        ["env", {
        "targets": {
                "browsers": [
                    "last 4 versions",
                    "Explorer 11"
                ]
            }
        }]
    ],
    "plugins": [
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }],
        ["module-resolver", {
            "root": ["./source/ts"],
            "alias": {
                ".": "/"
            }
        }]
    ]
}